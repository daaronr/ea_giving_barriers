@article{grossmanBeliefsSocialPreferences2008a,
  title = {Beliefs and {{Social Preferences}}},
  author = {Grossman, Zachary},
  year = {2008},
  abstract = {A person's willingness to sacrifice for others is highly sensitive to informational conditions, but it is unclear why. I conceptually and experimentally distinguish between three leading explanations, all based upon beliefs about the giver, which previous work has not adequately distinguished. 1) Social-signaling holds that a person wants others to think of her as fair- minded, 2) self-signaling says she would like to think of herself that way, and 3) beliefs-based altruism says that she cares about how her action makes another person feel. I present a model in which the decision-maker signals her preferences to an observer, which can correspond to each of these motivations. I experimentally test these interpretations using the model's predictions about a dictator game in which the outcome may be determined either by the dictator's choice or by chance. Limiting the recipient's ability to infer the dictator's choice lowers the frequency of giving, which is evidence of either social-signaling or beliefs-based altruism. However, I find little willingness to pay to in uence a recipient's beliefs about how he was treated by a third-party dictator, providing little support for belief-based altruism, and revealing partial information about the dictator's identity, while holding constant the recipient's information about his treatment, increases the frequency and amount of giving, suggesting social-signaling. Finally, lowering the expected cost of the decision to give by decreasing the likelihood that it will be implemented has little effect on dictator giving when the beliefs of the recipient are held constant, offering little support for self-signaling. This suggests that the act of giving is largely tied to what it says to others about the giver, as opposed to the self, or how it makes the recipient feel.},
  keywords = {altruism,behavioral,colleagues,dictator games,experiments,philanthropy,reputation,self-signaling,signaling,social giving}
}

