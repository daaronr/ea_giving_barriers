@article{adenaGivingOnceGiving2019,
  title = {Giving Once, Giving Twice: {{A}} Two-Period Field Experiment on Intertemporal Crowding in Charitable Giving},
  author = {Adena, Maja and Huck, Steffen},
  year = {2019},
  volume = {172},
