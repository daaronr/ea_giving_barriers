@article{coricelliPartnerSelectionPublic2004a,
  title = {Partner {{Selection}} in {{Public Goods Experiments}}},
  author = {Coricelli, Giorgio and Fehr, Dietmar and Fellner, Gerlinde},
  year = {2004},
