@article{edwardsUnderstandingWhySuggestions2014,
  ids = {Edwards2014,Edwards2014a,edwardsUnderstandingWhySuggestions2014a},
  title = {Toward an Understanding of Why Suggestions Work in Charitable Fundraising: Theory and Evidence from a Natural Field Experiment},
  author = {Edwards, James T. and List, John A.},
  year = {2014},
  month = jun,
  journal = {Journal of Public Economics},
  volume = {114},
  pages = {1--13},
  publisher = {{Elsevier B.V.}},
  issn = {0047-2727},
  doi = {10.1016/J.JPUBECO.2014.02.002},
  abstract = {People respond to those who ask. Within the charitable fundraising community, the power of the ask represents the backbone of most fundraising strategies. Despite this, the optimal design of communication strategies has received less formal attention. For their part, economists have recently explored how communication affects empathy, altruism, and giving rates to charities. Our study takes a step back from this literature to examine how suggestions\textendash a direct ask for a certain amount of money\textendash affect giving rates. We find that our suggestion amounts affect both the intensive and extensive margins: more people give and they tend to give the suggested amount. The resulting insights help us understand why people give, why messages work, and deepen practitioners' understanding of how to use messages to leverage more giving.},
  isbn = {00472727},
  keywords = {Charitable giving,Impure altruism,public economics,Public economics}
}

