@article{mugermanLongTermSavings2014,
  ids = {Mugerman2014,Mugerman2014a},
  title = {Long {{Term Savings Decisions}}: Financial {{Reform}}, {{Peer Effects}} and {{Ethnicity}} Sup*/Sup},
  author = {Mugerman, Yevgeny and Sade, Orly and Shayo, Moses},
  year = {2014},
  journal = {Journal of Economic Behavior \& Organization},
  publisher = {{Elsevier}},
  doi = {10.1016/j.jebo.2014.07.002},
  abstract = {Highlights ,\"A\textcent{} In 2005, a reform in the Israeli capital market shifted the power to choose savings vehicles from employers to individuals. ,\"A\textcent{} We study individual decisions following the reform using personnel data from a large employer. ,\"A\textcent{} Individual choice of fund was not associated with observable measures of fund performance, but was strongly affected by the choices of co-workers from the same ethnic group. Abstract In 2005, a drastic reform in the Israeli capital market shifted the power to choose savings vehicles from employers to individuals. Using a unique dataset from a large employer, this event provides us a rare window into individuals' savings decisions and the effect of their social environment. In the first year following the reform's implementation, 7\% of the employees switched out of the fund in which they all previously saved. Choice of fund was not associated with observable measures of fund performance, but was strongly affected by the employees' social environment. Exploiting within-department variation in peer groups, we find that savings decisions were strongly influenced by the choices of co-workers from the same ethnic group. Interviews also point to the influence of non-professional colleagues.}
}

