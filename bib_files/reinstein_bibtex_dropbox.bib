@article{Schmitz2019,
  title = {Temporal Dynamics of Pro-Social Behavior: An Experimental Analysis},
  author = {Schmitz, Jan},
  year = {2019},
  journal = {Experimental Economics},
  volume = {22},
  number = {1},
  issn = {13864157},
  doi = {10.1007/s10683-018-9583-2},
  abstract = {Individuals have opportunities to behave pro-socially at different points in time. This study investigates the interdependence between temporarily separated good deeds and their effect on individual pro-social behavior. In a multi-session laboratory experiment, subjects play a donation dictator game. The first group of subjects runs through two sessions on the same day. For the second group, there is a time-lag of one week between sessions. In both treatments, subjects decrease their donation decision in the second session. Spillover effects of pro-social behavior, however, decrease over time as the reduction in donations is smaller for subjects with larger time-lag between decisions.},
  keywords = {Behavioral spillovers,Charitable giving,Experimental economics,Social preferences}
}

@article{schmitzCharitableGivingZero2016,
  ids = {Schmitz2016,Schmitz2016a,Schmitz2016b},
  title = {Is {{Charitable Giving}} a {{Zero Sum Game}}? - {{The Effect}} of {{Competition Between Charities}} on {{Giving Behavior}}},
  author = {Schmitz, Jan},
  year = {2016},
  journal = {Ssrn},
  doi = {10.2139/ssrn.2862479},
  abstract = {The competition for donations between charities is tough. Yet, little is known about how people react to the extensive supply of charitable organizations. Do people have a need to satisfy their demand for giving by contributing to a particular charity? Or can the demand for doing good be satisfied by giving to any organization? In a donation dictator game competition between charities is simulated by altering the set of similar real charities to which subjects can give. I find weak substitution between charities when giving to more than one charity is possible, as the donated amounts to individual charities decrease with the size of the choice set. At the same time, the sum of donations to all charities increases when charities are in competition. Additionally, a reduced price in giving to one charity (matching grant) increases giving to the matched charity. However, a match does not result in a complete shift of donations to this charity. Further, subjects substitute part of their own contributions with the matching grant and leave their individual total contributions unchanged.},
  keywords = {C91,Charitable giving,Charitable Giving,competition,Competition,D64,Experimental Economics,H41,P43,social preferences,Social Preferences},
  file = {/Users/yosemite/Zotero/storage/CCPX2UGE/schmitz-2019-SSRN-id2862479.pdf}
